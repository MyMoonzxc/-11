#include<stdio.h>
int main() {
    int  n, A, count = 0;

    printf("n = ");
    scanf("%d", &n);

    int arr[100];

    printf("array = ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("A = ");
    scanf("%d", &A);

    for (int i = 0; i < n; i++) {
        if (arr[i] > A) count++;
    }

    printf("count = %d", count);

    return 0;
}
